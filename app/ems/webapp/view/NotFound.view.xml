<mvc:View
    controllerName="ems.controller.NotFound"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    
    <Page id="notFoundPage"
          showHeader="false">
        <content>
            <core:Fragment
                fragmentName="ems.view.fragment.Header"
                type="XML"
            />
            <IllustratedMessage
                id="notFoundMessage"
                illustrationType="sapIllus-PageNotFound"
                title="{i18n>notFoundTitle}"
                description="{i18n>notFoundText}">
                <additionalContent>
                    <Button
                        text="{i18n>backToHome}"
                        press="onNavToHome"
                        type="Emphasized" />
                </additionalContent>
            </IllustratedMessage>
        </content>
        <footer>
            <core:Fragment
                fragmentName="ems.view.fragment.Footer"
                type="XML"
            />
        </footer>
    </Page>
</mvc:View>